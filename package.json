{
  "name": "project-m",
  "productName": "Project M(anga)",
  "version": "1.0.0",
  "description": "An open source Manga Reader",
  "main": "main.js",
  "scripts": {
    "start": "electron ./app/",
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --retries 2 --compilers js:babel-register --recursive --require ./tests/setup.js ./tests/**/*.spec.js",
    "coverage": "istanbul cover -x \"**/bower_components/**\" _mocha -- --compilers js:babel-register --recursive --require ./tests/setup.js ./tests/**/*.spec.js && cat ./coverage/lcov.info | cross-env COVERALLS_REPO_TOKEN=PzYr7S8YrDosMyQIKfCN4iHEmAP9MNJAX ./node_modules/coveralls/bin/coveralls.js && rm -rf coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/na-ji/project-m.git"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron",
    "mocha": "./node_modules/.bin/mocha",
    "_mocha": "./node_modules/.bin/_mocha",
    "cross-env": "./node_modules/.bin/cross-env",
    "istanbul": "./node_modules/istanbul/lib/cli.js"
  },
  "keywords": [
    "Electron",
    "Manga",
    "Read",
    "open"
  ],
  "author": {
    "name": "Naji Astier",
    "email": "naji@astier.eu",
    "url": "https://www.naji-astier.com/"
  },
  "semistandard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "it",
      "before"
    ]
  },
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/na-ji/project-m/issues"
  },
  "homepage": "https://github.com/na-ji/project-m",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-polyfill": "^6.16.0",
    "chai": "^3.5.0",
    "chai-datetime": "^1.4.1",
    "coveralls": "^2.11.15",
    "cross-env": "^3.1.3",
    "electron-prebuilt": "^1.2.2",
    "electron-rebuild": "^1.1.5",
    "eslint": "^3.12.2",
    "eslint-config-semistandard": "^7.0.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "grunt": "^1.0.1",
    "grunt-contrib-less": "^1.3.0",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-symbolic-link": "^0.3.1",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "9.9.1",
    "jsdom-global": "^2.1.1",
    "mocha": "^3.1.2",
    "mocha-lcov-reporter": "^1.2.0"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "js-yaml": "^3.7.0",
    "lodash": "^4.17.3",
    "moment": "^2.17.1",
    "promise": "^7.1.1",
    "react": "^15.4.1",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "react-router-bootstrap": "^0.23.1",
    "request": "^2.79.0"
  }
}
