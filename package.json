{
  "name": "shogun",
  "productName": "Shogun Manga Reader",
  "version": "0.1.0",
  "description": "An open source Manga Reader",
  "main": "main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron ./app/",
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 jest '(\\\\|/)test(\\\\|/)(?!.*e2e\\.spec\\.js).+\\.spec\\.js'",
    "coverage": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 jest --coverage '(\\\\|/)test(\\\\|/)(?!.*e2e\\.spec\\.js).+\\.spec\\.js' && cat ./coverage/lcov.info | cross-env COVERALLS_REPO_TOKEN=PzYr7S8YrDosMyQIKfCN4iHEmAP9MNJAX ./node_modules/coveralls/bin/coveralls.js && rm -rf coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/na-ji/project-m.git"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron",
    "cross-env": "./node_modules/.bin/cross-env"
  },
  "keywords": [
    "Electron",
    "Manga",
    "Read",
    "open"
  ],
  "author": {
    "name": "Naji Astier",
    "email": "naji.astier@gmail.com",
    "url": "https://www.naji-astier.com/"
  },
  "semistandard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "it",
      "before"
    ]
  },
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/na-ji/project-m/issues"
  },
  "homepage": "https://github.com/na-ji/project-m",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-stage-0": "^6.22.0",
    "coveralls": "^2.11.15",
    "cross-env": "^3.1.3",
    "devtron": "^1.4.0",
    "electron-devtools-installer": "^2.1.0",
    "electron-prebuilt": "^1.2.2",
    "electron-rebuild": "^1.1.5",
    "eslint": "^3.12.2",
    "eslint-config-semistandard": "^7.0.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "flow-bin": "^0.39.0",
    "grunt": "^1.0.1",
    "grunt-contrib-less": "^1.3.0",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-symbolic-link": "^0.3.1",
    "jest": "^19.0.2",
    "jsdom": "9.11.0",
    "jsdom-global": "^2.1.1",
    "redux-logger": "^2.8.1"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-register": "^6.22.0",
    "cheerio": "^0.22.0",
    "electron-debug": "^1.1.0",
    "js-yaml": "^3.8.1",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "promise": "^7.1.1",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-list": "^0.8.3",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-bootstrap": "^0.23.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "request": "^2.79.0"
  }
}
